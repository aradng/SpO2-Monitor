 206:Core/Inc/ssd1306/ssd1306.c **** //    Draw one pixel in the screenbuffer
 207:Core/Inc/ssd1306/ssd1306.c **** //    X => X Coordinate
 208:Core/Inc/ssd1306/ssd1306.c **** //    Y => Y Coordinate
 209:Core/Inc/ssd1306/ssd1306.c **** //    color => Pixel color
 210:Core/Inc/ssd1306/ssd1306.c **** void ssd1306_DrawPixel(uint8_t x, uint8_t y, SSD1306_COLOR color) {
 211:Core/Inc/ssd1306/ssd1306.c ****     if(x >= SSD1306_WIDTH || y >= SSD1306_HEIGHT) {
 212:Core/Inc/ssd1306/ssd1306.c ****         // Don't write outside the buffer
 213:Core/Inc/ssd1306/ssd1306.c ****         return;
 214:Core/Inc/ssd1306/ssd1306.c ****     }
 215:Core/Inc/ssd1306/ssd1306.c ****     
 216:Core/Inc/ssd1306/ssd1306.c ****     // Check if pixel should be inverted
 217:Core/Inc/ssd1306/ssd1306.c ****     if(SSD1306.Inverted) {
 218:Core/Inc/ssd1306/ssd1306.c ****         color = (SSD1306_COLOR)!color;
 219:Core/Inc/ssd1306/ssd1306.c ****     }
 220:Core/Inc/ssd1306/ssd1306.c ****     
 221:Core/Inc/ssd1306/ssd1306.c ****     // Draw in the right color
 222:Core/Inc/ssd1306/ssd1306.c ****     if(color == White) {
 223:Core/Inc/ssd1306/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] |= 1 << (y % 8);
 224:Core/Inc/ssd1306/ssd1306.c ****     } else { 
 225:Core/Inc/ssd1306/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] &= ~(1 << (y % 8));
 226:Core/Inc/ssd1306/ssd1306.c ****     }
 227:Core/Inc/ssd1306/ssd1306.c **** }
