 200:Core/Inc/ssd1306/ssd1306.c ****         ssd1306_WriteCommand(0x00 + 2);     //if sh1106 add + 2
 201:Core/Inc/ssd1306/ssd1306.c ****         ssd1306_WriteCommand(0x10);
 202:Core/Inc/ssd1306/ssd1306.c ****         ssd1306_WriteData(&SSD1306_Buffer[SSD1306_WIDTH*i],SSD1306_WIDTH);
 203:Core/Inc/ssd1306/ssd1306.c ****     }
