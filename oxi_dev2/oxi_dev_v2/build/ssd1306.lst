 229:Core/Inc/ssd1306/ssd1306.c **** // Draw 1 char to the screen buffer
 230:Core/Inc/ssd1306/ssd1306.c **** // ch       => char om weg te schrijven
 231:Core/Inc/ssd1306/ssd1306.c **** // Font     => Font waarmee we gaan schrijven
 232:Core/Inc/ssd1306/ssd1306.c **** // color    => Black or White
 233:Core/Inc/ssd1306/ssd1306.c **** char ssd1306_WriteChar(char ch, FontDef Font, SSD1306_COLOR color) {
 234:Core/Inc/ssd1306/ssd1306.c ****     uint32_t i, b, j;
 235:Core/Inc/ssd1306/ssd1306.c ****     
 236:Core/Inc/ssd1306/ssd1306.c ****     // Check if character is valid
 237:Core/Inc/ssd1306/ssd1306.c ****     if (ch < 32 || ch > 126)
 238:Core/Inc/ssd1306/ssd1306.c ****         return 0;
 239:Core/Inc/ssd1306/ssd1306.c ****     
 240:Core/Inc/ssd1306/ssd1306.c ****     // Check remaining space on current line
 241:Core/Inc/ssd1306/ssd1306.c ****     if (SSD1306_WIDTH < (SSD1306.CurrentX + Font.FontWidth) ||
